{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "allowJs": false,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "types": ["bun-types"]
  },
  "include": ["server/**/*"]
}
